<%- include('partials/_header') -%>
        <div class="back_re">
         <div class="container">
            <div class="row">
               <div class="col-md-12">
                  <div class="title">
                   <h2>Our Trainers</h2>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <!-- trainers -->
      <div  class="trainers">
         <div class="container">
            <div class="row">
               <div class="col-md-12">
                  <div class="titlepage">
                     <p>Lorem Ipsum available, but the majority have suffered </p>
                  </div>
               </div>
            </div>
            <div class="row">
               <% for(let i = 0; i< trainers.length; i++ ){ %>
               <div class="col-md-4">
                  <div id="trai" class="trainer_box">
                     <div class="trainer_img">
                        <figure><img src="https://picsum.photos/200/100" alt="#"/></figure>
                     </div>
                     <div class="trainer">
                        <h3><%= trainers[i].name %></h3>
                        <h2>Courses</h2>
                        <% for(let x = 0; x < courses.length; x++ ){ %>
                           <% if(courses[x].user.name === trainers[i].name){ %>
                              <a href="/courses/<%= courses[x].slug %>"><%= courses[x].name %></a>
                           <% } %>
                        <% } %>
                     </div>
                  </div>
               </div>
               <% } %>
            </div>
         </div>
      </div>
      <!-- end trainers -->
<%- include('partials/_footer') -%>
